  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "webpack-dashboard -t 'Marvin' -- webpack-dev-server",
    "watch": "webpack",
    "build": "npm run clean && NODE_ENV=\"production\" webpack",
    "preview": "npm run clean && NODE_ENV=\"production\" webpack-dev-server",
    "clean": "rm -rf ./build",
    "lint-hook": "eslint ./source/js ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint": "npm run lint-hook --silent || true",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "test": "npm run lint",
    "manage:translations": "node ./translations.config.js"
  },
