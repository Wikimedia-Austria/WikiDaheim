    "create-react-class": "^15.5.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "ftp-deploy": "1.x",
    "geolib": "^3.0.4",
    "gh-pages": "^2.0.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "js-base64": "^2.5.1",
    "mapbox-gl": "^1.0.0",
    "promise-ftp-common": "^1.1.5",
    "prop-types": "^15.7.2",
