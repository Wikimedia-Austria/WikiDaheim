{
  "name": "WikiDaheim",
  "version": "0.1.0",
  "private": true,
  "license": "(cc) by-sa 2017 Benedikt Reiter",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "client:dev": "webpack-dashboard -t 'Marvin' -- webpack-dev-server",
    "client:watch": "webpack",
    "client:build": "npm run clean && NODE_ENV=\"production\" webpack",
    "client:preview": "npm run clean && NODE_ENV=\"production\" webpack-dev-server",
    "server:watch": "SERVER_RENDER=\"true\" webpack --config webpack.config.server.js",
    "server:restart": "nodemon ./build/server.js",
    "server:build": "SERVER_RENDER=\"true\" NODE_ENV=\"production\" webpack --config webpack.config.server.js",
    "server:dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"webpack-server,watch-server\" --kill-others \"npm run server:watch\" \"npm run server:restart\"",
    "server-and-client:dev": "concurrently --prefix \"[{name}]\" --names \"webpack-server,webpack-client,watch-server\" --kill-others \"npm run server:watch\" \"npm run client:watch\" \"npm run server:restart\"",
    "server-and-client:build": "npm run build-client && npm run build-server",
    "clean": "rm -rf ./build",
    "lint-hook": "eslint ./source/js ./webpack ./*.js -f table --ext .js --ext .jsx",
    "lint": "npm run lint-hook --silent || true",
    "hook-add": "prepush install",
    "hook-remove": "prepush remove",
    "deploy": "gh-pages -d build/",
    "test": "npm run lint"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.6.1",
    "concurrently": "^3.4.0",
    "css-loader": "0.14.5",
    "eslint": "^3.10.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^3.13.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.1.1",
    "prepush": "^3.1.11",
    "react-hot-loader": "^3.0.0-beta.7",
    "redux-logger": "^2.7.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.0",
    "svg-sprite-loader": "^2.1.0",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.2.1",
    "worker-loader": "^0.8.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "es6-promise": "^3.3.1",
    "express": "^4.15.3",
    "geolib": "^2.0.22",
    "gh-pages": "^1.0.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "js-md5": "^0.5.0",
    "mapbox-gl": "^0.38.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-autocomplete": "^1.5.9",
    "react-dom": "^15.5.4",
    "react-infinite": "^0.11.0",
    "react-mapbox-gl": "^2.1.0",
    "react-redux": "^4.4.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-truncate": "^2.1.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0"
  },
  "description": "WikiDaheim Client for Wikimedia Austria",
  "author": "Benedikt Reiter",
  "prepush": [
    "npm run lint-hook --silent"
  ]
}
